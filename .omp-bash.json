{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0 ",
          "template": "{{ if .WSL }}\uf17c{{ end }}",
          "foreground": "#FFFFFF",
          "background": "#26C6DA",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0 ",
          "template": "\ue0a2 ",
          "foreground": "#FFFFFF",
          "background": "#D75F00",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0 ",
          "template": "{{ if .SSHSession }}\uf066 {{ end }}",
          "foreground": "#FFFFFF",
          "background": "#7E46B6",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "\ue725 ",
            "branch_identical_icon": "",
            "cache_duration": "none",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "fetch_status": true,
            "merge_icon": "",
            "rebase_icon": "",
            "revert_icon": "",
            "tag_icon": ""
          },
          "template": "({{ .HEAD }}){{ if .Staging.Changed }}<#87FF00> {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> {{ .Working.String }}</>{{ end }} ",
          "foreground": "#5FD7FF",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0 ",
          "template": " \ue70c {{ .Full }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#00ffff",
          "type": "dotnet",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0 ",
          "template": " \ue235 {{ .Full }}",
          "foreground": "#100e23",
          "background": "#dfd5ff",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "environment"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0 ",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#100e23",
          "background": "#906cff",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if ne \"ktos\" .UserName}}<u>{{ .UserName }}</u>{{ end }}",
          "foreground": "#d70000",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if eq \"ktos\" .UserName}}{{ .UserName }}{{ end }}",
          "foreground": "#b5ff6a",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "@{{ .HostName }}",
          "foreground": "#b5ff6a",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": ":",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "{{ .Path }}",
          "foreground": "#3b78ff",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if (or (eq \"pwsh\" .Name) (eq \"cmd\" .Name)) }}>{{else}}$ {{end}}",
          "foreground": "#FFFFFF",
          "type": "shell",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
